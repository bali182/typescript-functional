{
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "module": "commonjs",
        "target": "ES5",
        "outDir": "src-gen",
        "out": "src-gen/app.js",
        "removeComments": false,
        "suppressImplicitAnyIndexErrors": false,
        "noEmitOnError": true
    }
}